{
  "name": "nestjs-app-api",
  "version": "0.0.1",
  "description": "A NestJs base project with Knexjs and Sequelize",
  "author": {
    "name": "thuanlegit",
    "email": "dthuanle.20@gmail.com",
    "url": "https://github.com/thuanlegit"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "webpack --mode=development --watch --progress",
    "build": "rm -rf dist && webpack --mode=production",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:list:pg_main": "knex migrate:list --env pg_main",
    "migration:create:pg_main": "knex migrate:make -x ts --env pg_main",
    "migration:run:pg_main": "knex migrate:latest --env pg_main",
    "migration:up:pg_main": "knex migrate:up --env pg_main",
    "migration:down:pg_main": "knex migrate:down --env pg_main",
    "migration:rollback:pg_main": "knex migrate:rollback --env pg_main",
    "migration:list": "pnpm run migration:list:pg_main",
    "migration:run": "pnpm run migration:run:pg_main",
    "seed:create:pg_main": "knex seed:make -x ts --env pg_main",
    "seed:run:pg_main": "knex seed:run --env pg_main",
    "seed:run": "pnpm run seed:run:pg_main"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.0.3",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "knex": "^3.1.0",
    "nest-knexjs": "^0.0.26",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/body-parser": "^1.19.5",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-node-externals": "^3.0.4",
    "@types/webpackbar": "^4.0.6",
    "@typescript-eslint/parser": "^8.24.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon-webpack-plugin": "^4.8.2",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "@swc/core"
    ]
  }
}